{
  "expo": {
    "name": "Soudou",
    "slug": "soudou-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/soudou-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/soudou-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/soudou-logo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.yourusername.soudouapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
